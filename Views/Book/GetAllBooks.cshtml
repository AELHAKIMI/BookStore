@model IEnumerable<BookModel>;

<div class="py-5 bg-light">
  <div class="container">
    <h3 class="h3">All Books</h3>
    <div class="row">
      @foreach (var book in Model)
      {
        <div class="col-md-4">
          <partial name="_BookThumbnail" model="book" />
        </div>
      }


    </div>
  </div>
</div>
